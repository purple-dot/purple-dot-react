{
    "env": {
      "browser": true,
      "es2020": true,
      "jest/globals": true,
      "cypress/globals": true
    },
  "plugins": ["jest", "cypress"],
    "extends": [
        "airbnb-base",
        "plugin:cypress/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "no-console": ["error", {"allow": ["warn", "error"]}]
    }
}
